{
  "extends": "tslint:latest",
  "rules": {
    "member-access": [
      false
    ],
    "arrow-parens": false,
    "interface-name": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "object-literal-sort-keys": false,
    "member-ordering": [
      false
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "no-unused-expression": true,
    "no-unused-variable": [
      true
    ]
  }
}
